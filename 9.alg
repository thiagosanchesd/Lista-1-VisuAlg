algoritmo "9"
var
   l1, l2, l3, volume: real

inicio
   escreva("Informe o comprimento do prisma: ")
   leia(l1)
   escreva("Informe a largura do prisma: ")
   leia(l2)
   escreva("Informe a altura do prisma: ")
   leia(l3)
   volume := l1 * l2 * l3
   escreva("Volume do prisma: ", volume:4:2)
fimalgoritmo






