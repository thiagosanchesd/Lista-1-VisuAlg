Algoritmo "Parte10"
var
   numero1, numero2, resultado: real
inicio

   escreva("Digite o primeiro número: ")
   leia(numero1)
   escreva("Digite o segundo número: ")
   leia(numero2)


   resultado <- numero1 - numero2
   
   resultado <- resultado/10


   escreva("A décima parte da subtração é: ", resultado)
fimalgoritmo

